{
    "name": "team/teamtoolbox",
    "license": "MIT",
    "type": "project",
    "description": "Team Toolbox",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "replace": {
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php81": "*"
    },
    "require": {
        "php": "^8.2",
        "ext-gd": "*",
        "ext-pdo_sqlite": "*",
        "bacon/bacon-qr-code": "^2.0",
        "cmen/google-charts-bundle": "^5.0",
        "composer/package-versions-deprecated": "^1.11",
        "doctrine/annotations": "^2.0",
        "doctrine/dbal": "^3.6",
        "doctrine/doctrine-bundle": "^2.10",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.15",
        "erusev/parsedown": "^1.7",
        "imagine/imagine": "^1.3",
        "league/oauth2-client": "^2.7",
        "mashape/unirest-php": "^3.0",
        "microsoft/microsoft-graph": "^1.100",
        "mpratt/embera": "~2.0",
        "phpoffice/phpspreadsheet": "^1.29",
        "sensio/framework-extra-bundle": "^6.2",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "^6.4",
        "symfony/console": "^6.4",
        "symfony/dotenv": "^6.4",
        "symfony/event-dispatcher": "^6.4",
        "symfony/expression-language": "^6.4",
        "symfony/flex": "^2.3",
        "symfony/form": "^6.4",
        "symfony/framework-bundle": "^6.4",
        "symfony/html-sanitizer": "^6.4",
        "symfony/http-client": "^6.4",
        "symfony/intl": "^6.4",
        "symfony/mailer": "^6.4",
        "symfony/monolog-bundle": "^3.8",
        "symfony/polyfill-intl-messageformatter": "^1.27",
        "symfony/security-bundle": "^6.4",
        "symfony/serializer": "^6.4",
        "symfony/string": "^6.4",
        "symfony/translation": "^6.4",
        "symfony/twig-bundle": "^6.4",
        "symfony/validator": "^6.4",
        "symfony/webpack-encore-bundle": "^1.17",
        "symfony/yaml": "^6.4",
        "twig/extra-bundle": "^3.6",
        "twig/intl-extra": "^3.6",
        "twig/markdown-extra": "^3.6"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^7.2",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "symfony/browser-kit": "^6.4",
        "symfony/css-selector": "^6.4",
        "symfony/debug-bundle": "^6.4",
        "symfony/maker-bundle": "^1.49",
        "symfony/phpunit-bridge": "^6.4",
        "symfony/stopwatch": "^6.4",
        "symfony/web-profiler-bundle": "^6.4",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-doctrine": "^1.3",
        "phpstan/phpstan-symfony": "^1.3"
    },
    "config": {
        "platform": {
            "php": "8.2"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true,
            "phpstan/extension-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        },
        "post-install-cmd": [],
        "post-update-cmd": []
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "require": "6.4.*",
            "allow-contrib": true
        }
    }
}
