[
{% set i = 0 %}
{% for userDate in userDates %}{% set i = i+1 %}
    {
        {% if not onlyType %}"title": "{% if userDate.collab is not null %}{{ userDate.collab.fullName }}{% endif %} ({{ userDate.version }})",{% else %}"title": "MEP {{ userDate.version }}",{% endif %}
        {% if userDate.collab is not null %}"url": "{{ path('collab_index', {userId: userDate.collab.id}) }}",{% endif %}
        "start": "{{ userDate.startAt|format_datetime(pattern="yyyy-MM-dd") }}"{% if userDate.endAt %},
        "end": "{{ userDate.endAt|format_datetime(pattern="yyyy-MM-dd") }}"{% endif %},
        "icon": "fa-truck-loading"
    }{% if i != userDates|length %},{% endif %}
{% endfor %}
]