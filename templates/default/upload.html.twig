{% extends 'basev2.html.twig' %}

{% block body_id 'upload_everwin' %}

{% block title %}Import RHPI{% endblock %}

{% block current_page_name %}
    Import RHPI
{% endblock %}

{% block contextual_menu %}
{% endblock %}

{% block contentHeaderTitle %}<h1 class="m-0">Import RHPI</h1>{% endblock %}
{% block breadcrumb %}{% endblock %}

{% block body %}
    {% for message in app.flashes('success') %}
        <div class="alert alert-primary" role="alert">
            {{ message }}
        </div>
    {% endfor %}

    {{ form_start(form) }}
        {{ form_widget(form) }}

        <button type="submit" class="btn btn-primary">
            <i class="fa fa-save" aria-hidden="true"></i> {{ 'action.save'|trans }}
        </button>
    {{ form_end(form) }}
{% endblock %}