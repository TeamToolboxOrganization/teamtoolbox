<div class="container"  style="width: auto;">
    {% if showTitle is defined and showTitle %}
        {% include 'widget/titleSection.html.twig' with {'icon': 'far fa-comments', 'iconColor': 'text-primary', 'title': 'Les derniers One-to-One'} %}
    {% endif %}
    <div class="row">
        <div class="col-sm-12">
            <div class="timeline">
                {% for note in allNotes %}

                    <div class="time-label">
                        <span class="bg-red">{{ note.publishedAt|format_datetime('short', 'none', locale='fr') }}</span>
                    </div>

                    <div>
                        {% if note.mindset is defined and note.mindset%}
                            {% include 'widgetv2/mindsetFont.html.twig' with {'value': note.mindset.value} %}
                        {% else %}
                            <i class="fas fa-comments bg-yellow"></i>
                        {% endif %}

                        <div class="timeline-item">
                            <span class="time"><i class="fas fa-clock"></i> {{ note.publishedAt|format_datetime('none', 'short', locale='fr') }}</span>
                            <h3 class="timeline-header">
                                {% if showChecked is defined and note.checked is defined %}
                                    <div class="small text-gray">
                                        <input id="check_{{ note.id }}" type="checkbox" onclick="checkNote('{{ note.id }}');" value="{% if note.checked %}true{% else %}false{% endif %}"{% if note.checked %} checked{% endif %}> Sujet discut√©
                                    </div>
                                {% endif %}
                                {{ note.type }}
                            </h3>
                            <div class="timeline-body" name="readNoteNode" id="{{ note.id }}">
                            </div>
                            <div class="timeline-footer">
                            </div>
                        </div>
                    </div>

                {% endfor %}
            </div>
